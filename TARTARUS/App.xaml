<Application x:Class="TARTARUS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TARTARUS"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/TextBoxTheme.xaml"/>
                <ResourceDictionary Source="Themes/ButtonReloadTheme.xaml"/>
                <ResourceDictionary Source="Themes/ButtonHomeTheme.xaml"/>
                <ResourceDictionary Source="Themes/ButtonForwardTheme.xaml"/>
                <ResourceDictionary Source="Themes/ButtonBackTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
